# Server Configuration
PORT=3000
API_BASE_URL=http://localhost:3000

# Google Gemini API Key (Replace with your actual key)
# Get one at https://ai.google.dev/tutorials/setup
# Must have access to the "gemini-2.0-flash" model with Search grounding feature
GOOGLE_GEMINI_API_KEY=your_gemini_api_key_here

# App Configuration
NODE_ENV=development

# Tunnel Mode Configuration (Uncomment and fill when using tunnel mode)
# PUBLIC_BACKEND_URL=https://your-deployed-backend-url.com
# Set this to true when deploying the backend for tunnel mode
# USE_TUNNEL=false 